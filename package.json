    "db:seed:one": "tsx db/seed/seedRunner.ts",
    "db:seed:one:fresh": "tsx db/seed/seedRunner.ts --fresh",
    "db:seed:create": "tsx db/seed/createSeeder.ts",
    "shell": "docker exec -it ext-web-next sh",
    "permission": "sudo chmod 777 -R .",
    "ngrock": "sh ngRock.sh",
    "start:cron": "tsx server/cron/cron.ts",
    "remoteOpen": "sudo chmod +x ./scripts/openRemoteVscode.sh && ./scripts/local/openRemoteVscode.sh",
    "remoteDeploy": "sudo chmod +x ./scripts/local/remoteDeploy.sh && ./scripts/local/remoteDeploy.sh"
